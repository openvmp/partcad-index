{
  "files.trimFinalNewlines": true,
  "files.encoding": "utf8",
  "editor.useTabStops": false,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.autoIndent": "full",
  "editor.rulers": [
    80
  ],
  "files.exclude": {
    ".git/**": true,
    "**/__pycache__": true,
    "**/apache20.svg": true,
    "**/LICENSE.txt": true,
    "**/.partcad": true,
    "build": true
  }
}